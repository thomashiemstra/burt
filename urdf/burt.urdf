<robot name="pi_robot">

	<!-- * * * Link Definitions * * * -->

 	<link name="base_link">
 	    <inertial>
          <origin rpy="0 0 0" xyz="0 0 0"/>
          <mass value=".0"/>
          <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
        </inertial>
		<visual>
		    <origin xyz="0 0 0.0425" rpy="0 0 0"/>
			<geometry>
				<box size="0.5 0.2 0.1"/>
			</geometry>
			<material name="Cyan1">
	       		<color rgba="0 0.9 0.9 1.0"/>
	     	</material>
		</visual>
	</link>

 	<link name="arm_1">
		<visual>
		    <origin xyz="0 0.2 0" rpy="0 0 0"/>
			<geometry>
				<box size="0.1 0.5 0.1"/>
			</geometry>
			<material name="Black1">
	       		<color rgba="0.2 0.2 0.2 1.0"/>
	     	</material>
		</visual>
	</link>

  <joint name="base_to_arm_1" type="revolute">
    <limit effort="1000.0" lower="-1.57079632679" upper="3.14159265359" velocity="10"/>
    <parent link="base_link"/>
    <child link="arm_1"/>
    <origin rpy="0 0 0" xyz="-0.2 0.0 0.15"/>
    <axis xyz="0 0 1"/>
  </joint>

   	<link name="arm_1_1">
		<visual>
		    <origin xyz="0 0.2 0" rpy="0 0 0"/>
			<geometry>
				<box size="0.1 0.5 0.1"/>
			</geometry>
			<material name="Black1">
	       		<color rgba="0.2 0.2 0.2 1.0"/>
	     	</material>
		</visual>
	</link>

      <joint name="arm_1_to_arm_1_1" type="continuous">
        <limit effort="1000.0" lower="-1.57079632679" upper="3.14159265359" velocity="10"/>
        <parent link="arm_1"/>
        <child link="arm_1_1"/>
        <origin rpy="0 0 0" xyz="0 0.4 -0.1"/>
        <axis xyz="0 0 1"/>
      </joint>

    <link name="arm_2">
		<visual>
		    <origin xyz="0 0.2 0" rpy="0 0 0"/>
			<geometry>
				<box size="0.1 0.5 0.1"/>
			</geometry>
			<material name="Black1">
	       		<color rgba="0.2 0.2 0.2 1.0"/>
	     	</material>
		</visual>
	</link>

  <joint name="base_to_arm_2" type="continuous">
    <limit effort="1000.0" lower="-1.57079632679" upper="3.14159265359" velocity="10"/>
    <parent link="base_link"/>
    <child link="arm_2"/>
    <origin rpy="0 0 0" xyz="0.2 0.0 0.15"/>
    <axis xyz="0 0 1"/>
  </joint>

   	<link name="arm_2_1">
		<visual>
		    <origin xyz="0 0.2 0" rpy="0 0 0"/>
			<geometry>
				<box size="0.1 0.5 0.1"/>
			</geometry>
			<material name="Black1">
	       		<color rgba="0.2 0.2 0.2 1.0"/>
	     	</material>
		</visual>
	</link>

      <joint name="arm_2_to_arm_2_1" type="continuous">
        <limit effort="1000.0" lower="-1.57079632679" upper="3.14159265359" velocity="10"/>
        <parent link="arm_2"/>
        <child link="arm_2_1"/>
        <origin rpy="0 0 0" xyz="0 0.4 -0.1"/>
        <axis xyz="0 0 1"/>
      </joint>


</robot>